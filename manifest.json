{
    "name": "Jnote",
    "version": "1.0",
    "manifest_version": 2,
    "description": "A simple and convenient  web note",
    "browser_action": {
        "default_icon": "image/icon.png",
        "default_popup": "page/popup.html"
    },
    "permissions": [
        "http://www.baidu.com"
    ],
    "content_scripts": [
        {
            "matches": [
            "http://*/*",
            "https://*/*"
        ],
            "js": [
              "script/jquery.min.js",
              "script/myscript.js"
            ],
            "css": [
                "style/mycss.css"
            ]
        }
      ]
}